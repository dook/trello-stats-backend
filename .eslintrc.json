{
  "extends": ["eslint:recommended", "plugin:node/recommended", "plugin:import/errors", "plugin:import/warnings"],
  "env" : {
      "node" : true,
      "es6" : true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules" : {
    "no-unused-vars" : 1,
    "no-undef" : 2,
    "node/no-unsupported-features/es-syntax": 0,
    "quotes": ["error", "single"],
    "semi": ["error", "never"]
  }
}
